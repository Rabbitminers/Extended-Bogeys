{
  "schemaVersion": 1,
  "id": "extendedbogeys",
  "version": "${version}",
  "name": "Extended Bogeys",
  "description": "An addon mod to Create adding new styles of bogey and an interface for other mods to do so",
  "authors": [
    "Rabbitminers",
    "RailFanNate",
    "Keanes2",
    "Matt.",
    "kurocat471"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "GNU GPLv3",
  "icon": "assets/examplemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.rabbitminers.extendedbogeys.fabric.ExtendedBogeysImpl"
    ],
    "client": [
      "com.rabbitminers.extendedbogeys.fabric.ExtendedBogeysClientImpl"
    ]
  },
  "mixins": [
    "extendedbogeys.mixins.json",
    "extendedbogeys-common.mixins.json"
